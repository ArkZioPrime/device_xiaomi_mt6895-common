type data_vendor_de_md_file, file_type, data_file_type;
type mcf_ota_file, file_type, data_file_type;

allow ccci_mdinit data_vendor_de_md_file:dir { ioctl read write create getattr setattr lock rename open watch watch_reads add_name remove_name reparent search rmdir };
